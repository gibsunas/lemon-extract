{
  "name": "@lemon/extract",
  "version": "0.2.11",
  "description": "",
  "type": "module",
  "es": "bin/cli.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:gibsunas/lemon-extract.git"
  },
  "bin": {
    "@lemon/extract": "bin/cli.cjs"
  },
  "engines": {
    "node": ">=14",
    "typescript": ">=4"
  },
  "engineStrict": true,
  "scripts": {
    "nodemon": "nodemon -e .js -w dist --exec 'webpack && bin/extract.cjs'",
    "webpack": "webpack -c webpack.config.cjs",
    "clean": "rm -rf dist/",
    "release:build": "npm run webpack && mkdir -p dist/bin && cp bin/cli.cjs dist/bin/cli.cjs && cp bin/extract.cjs dist/bin/extract.cjs",
    "postrelease:build": "echo 'Testing basic CLI execution' && node ./bin/extract.cjs -h && DEBUG=@lemon/extract:* node ./bin/extract.cjs something",
    "release:prepare": "npm run clean && npm run release:build && cd dist/ && node ../scripts/publishing/prepare.cjs",
    "build": "npm run webpack",
    "watch": "npm run webpack -- --watch",
    "cli": "node bin/extract.cjs",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "patch": "npm version patch && npm run release:prepare && cd dist/ && npm publish",
    "minor": "npm version minor && npm run release:prepare && cd dist/ && npm publish",
    "patch:dry": "npm run clean && npm version patch && npm run release:prepare && cd dist/ && npm publish --dry-run",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "lint": "eslint --fix ./"
  },
  "private": false,
  "author": "James Gibson",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.6",
    "@nrwl/workspace": "^11.5.2",
    "cli-table": "^0.3.1",
    "commander": "^2.20.3",
    "debug": "^4.3.2",
    "github-api": "^3.4.0",
    "glob": "^7.1.7",
    "node-fetch": "^2.0.0",
    "pretty-bytes": "^5.3.0",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.13.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.12.13",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-jest": "^27.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-jest": "^27.2.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.2",
    "jest-cli": "^27.2.5",
    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.9",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "2.5.2"
  }
}
