{
  "name": "@lemon/extract",
  "version": "0.0.20",
  "description": "",
  "type": "module",
  "es": "bin/extract.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:gibsunas/lemon-extract.git"
  },
  "bin": {
    "@lemon/extract": "bin/extract.js"
  },
  "engines": {
    "node": ">=14",
    "typescript": ">=4"
  },
  "engineStrict": true,
  "scripts": {
    "nodemon": "nodemon -e .js -w dist --exec 'webpack && bin/extract.js'",
    "webpack": "webpack -c webpack.config.cjs",
    "clean": "rm -rf dist/",
    "release:build": "mkdir -p dist/bin && tsc -b --clean && tsc -b --force && mkdir -p dist/bin && cp bin/extract.js dist/bin/extract.js",
    "release:prepare": "npm run clean && npm run release:build && cd dist/ && ts-node ../scripts/publishing/prepare.ts",
    "build": "mkdir -p dist && tsc -b",
    "run": "npm run webpack && bash bin/extract",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "patch": "npm version patch && npm run release:prepare && cd dist/ && npm publish",
    "patch:dry": "npm run clean && npm version patch && npm run release:prepare && cd dist/ && npm publish --dry-run",
    "test": "jest",
    "lint": "eslint"
  },
  "private": false,
  "author": "James Gibson",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.6",
    "@nrwl/workspace": "^11.5.2",
    "cli-table": "^0.3.1",
    "commander": "^2.20.3",
    "github-api": "^3.4.0",
    "glob": "^7.1.7",
    "pretty-bytes": "^5.3.0"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.2",
    "nodemon": "^2.0.9",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "2.5.2"
  }
}
